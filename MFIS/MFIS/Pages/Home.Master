    <script type="text/javascript">
        var u_s_id = '<%=Session["USERID"].ToString() %>';
        $(document).ready(function () {
            var u_s_id = '<%=Session["USERID"].ToString() %>';
            //alert(u_s_id);
            $.ajax({
                url: 'Script/home/admin/dashboard/User_Privileges.ashxd',
                method: 'get',
                dataType: 'json',
                data: { 'Id': u_s_id },
                success: function (data) {
                    buildMenu($('#menu'), data);
                    $('#menu').menu();
                },
                error: function (err) {
                }
            });

            function buildMenu(parent, items) {
                $.each(items, function () {
                    var li = $('<li class="treeview active"><a href="' + this.Url + '"><i class="fa fa-link"></i> <span>' + this.MenuText + '</span></a></li>');
                    li.appendTo(parent);

                    if (this.List && this.List.length > 0) {
                        var ul = $('<ul class="treeview-menu menu-open"></ul>');
                        ul.appendTo(li);
                        buildMenu(ul, this.List);
                    }
                });
            }
        });
        function goto_logout() {
            window.open("Script/frmLogin.aspx", "_self");
        }
        function goto_forget_password() {
            window.open("Script/home/admin/forgot_password/", "_self");
        }
        $(function () {
            user_name();
        });
        function user_name() {
            var u_s_id = '<%=Session["USERID"].ToString() %>';
            var u_s_name = '<%=Session["USER_NAME"].ToString() %>';
            var u_emp_id = '<%=Session["EMP_ID"].ToString() %>';
            $("#user_img_1").attr("src", "Script/system/images/icon/" + u_emp_id + ".jpg");
            $("#user_img_2").attr("src", "Script/system/images/icon/" + u_emp_id + ".jpg");
            $('#msg').html(u_s_name);
        }
    </script>

